(library
 (name ppx_rapper)
 (kind ppx_rewriter)
 (archives (byte ppx_rapper.cma) (native ppx_rapper.cmxa))
 (plugins (byte ppx_rapper.cma) (native ppx_rapper.cmxs))
 (native_archives ppx_rapper.a)
 (requires ppxlib.ast ppxlib pg_query base)
 (main_module_name Ppx_rapper)
 (modes byte native)
 (modules
  (wrapped
   (main_module_name Ppx_rapper)
   (modules
    ((name Codegen) (obj_name ppx_rapper__Codegen) (visibility public) (impl))
    ((name Ppx_rapper) (obj_name ppx_rapper) (visibility public) (impl))
    ((name Query)
     (obj_name ppx_rapper__Query)
     (visibility public)
     (impl)
     (intf)))
   (alias_module
    (name Ppx_rapper__)
    (obj_name ppx_rapper__)
    (visibility public)
    (kind alias)
    (impl))
   (wrapped true))))
